<h1><%= @project.title %></h1>


<%= form_for @project, :html => { :multipart => true }  do |f| %>
 <p>
	<%= f.label :title %><br>
	<%= f.text_field :title %>
</p>
 <p>
	<%= f.label :tweet %><br>
	<%= f.text_field :tweet %>
</p>
<div class="avatar">
	<p>
	<%= f.label :avatar %>
	<%= f.file_field :avatar %>
	</p>
</div>
 <p>
	<%= f.submit %>
</p>
<% end %>